{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noEmitHelpers": false,
    "sourceMap": true
  },
  "filesGlob": [
    "./**/*.ts",
    "./test/**/*.ts",
    "!./jspm_packages/**/*.ts",
    "!./node_modules/**/*.ts",
    "!./typings/**/*.ts",
    "./typings/tsd.d.ts"
  ],
  "compileOnSave": false,
  "buildOnSave": false,
  "atom": {
    "rewriteTsconfig": true
  },
  "files": [
    "./app.ts",
    "./bin/www.ts",
    "./io.ts",
    "./models/bullet.ts",
    "./models/math.ts",
    "./models/player.ts",
    "./models/team.ts",
    "./routes/index.ts",
    "./typings/tsd.d.ts"
  ]
}
